{
"namespace": "event.course_install",
"type": "record",
"name": "CourseInstall",
"doc": "The first version of the course install document",
"fields": [
        {"name": "course",
         "type": {
             "type": "record",
             "name": "Course",
             "fields": [
                {"name": "uuid", "type": "string"},
                {"name": "name", "type": "string", "aliases": ["courseName"]},
                {"name": "knownLanguage", "type": "string"},
                {"name": "targetLanguage", "type": "string"},
                {"name": "product", "type": "string"}
            ]
        }
        },
        {"name": "units",
         "type": {
            "type": "array",
            "items": {
                "type": "record",
                "name": "Unit",
                "fields": [
                    {"name": "id", "type": "int", "default": 0},
                    {"name": "uuid", "type": "string"},
                    {"name": "name", "type": "string"},
                    {"name": "maxScore", "type": "int"},
                    {"name": "minScore", "type": "int"},
                    {"name": "hasAssessment", "type": "boolean"},
                    {"name": "lessons",
                     "type": {
                        "type": "array",
                        "items": {
                            "type": "record",
                            "name": "Lesson",
                            "fields": [
                                {"name": "id", "type": "int"},
                                {"name": "uuid", "type": "string"},
                                {"name": "name", "type": "string"},
                                {"name": "position", "type": "int"},
                                {"name": "isAssessment", "type": "boolean"}
                                ]
                            }
                        }
                    }
                    ]
                }
            }
         }
    ]
}